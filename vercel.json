{
  "rewrites": [
    {
      "source": "/certificates/(.*)",
      "destination": "/certificates/$1"
    },
    {
      "source": "/resume/(.*)",
      "destination": "/resume/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ]
}